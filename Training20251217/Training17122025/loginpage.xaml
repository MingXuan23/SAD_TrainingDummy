<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Training17122025.loginpage"
             Title="loginpage">
    <VerticalStackLayout>
        <Entry Placeholder="Enter your email" x:Name="emailtxt"></Entry>
        <Entry Placeholder="Enter your password" x:Name="passwordtxt" IsPassword="True"></Entry>
        <Button Text="Login" Clicked="Button_Clicked"></Button>
    </VerticalStackLayout>
</ContentPage>